{
  "name": "react-template-next",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-circular": "npx madge --circular src",
    "output-dependencies": "npx madge --image dependencies.svg src",
    "remove-node_modules": "rimraf node_modules",
    "upgrade-node_modules": "npx npm-check-updates -u",
    "prebuild": "npm run check-circular && npm run lint",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix src"
    ],
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.3.4",
    "@mui/icons-material": "5.15.14",
    "@mui/material": "5.15.14",
    "@supercharge/promise-pool": "3.1.1",
    "@tanstack/react-query": "5.28.6",
    "ahooks": "3.7.10",
    "await-to-js": "3.0.0",
    "axios": "1.6.8",
    "dayjs": "1.11.10",
    "immer": "10.0.4",
    "lodash-es": "4.17.21",
    "qs": "6.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.13",
    "react-hook-form": "7.51.1",
    "react-router": "6.22.3",
    "react-router-dom": "6.22.3",
    "zod": "3.22.4",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.0.4",
    "@storybook/addon-interactions": "8.0.4",
    "@storybook/addon-links": "8.0.4",
    "@storybook/addon-onboarding": "8.0.4",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "8.0.4",
    "@storybook/react": "8.0.4",
    "@storybook/react-vite": "8.0.4",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@total-typescript/ts-reset": "0.5.1",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.30",
    "@types/qs": "6.9.14",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "@vitejs/plugin-react-swc": "3.6.0",
    "@vitest/coverage-v8": "1.4.0",
    "@vitest/ui": "1.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "happy-dom": "14.3.1",
    "husky": "9.0.11",
    "identity-obj-proxy": "3.0.0",
    "lint-staged": "15.2.2",
    "madge": "6.1.0",
    "npm-check-updates": "16.14.17",
    "picocolors": "1.0.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "rollup-plugin-visualizer": "5.12.0",
    "storybook": "8.0.4",
    "typescript": "5.4.3",
    "typescript-plugin-css-modules": "5.1.0",
    "vite": "5.2.2",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-node-polyfills": "0.21.0",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-singlefile": "2.0.1",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.4.0"
  }
}
