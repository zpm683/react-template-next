{
  "name": "react-template-next",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-circular": "npx madge --circular src",
    "output-dependencies": "npx madge --image dependencies.svg src",
    "remove-node_modules": "rimraf node_modules",
    "upgrade-node_modules": "npx npm-check-updates -u",
    "prebuild": "npm run check-circular && npm run lint"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix src"
    ],
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@supercharge/promise-pool": "3.0.0",
    "@tanstack/react-query": "4.32.1",
    "ahooks": "3.7.8",
    "axios": "1.4.0",
    "dayjs": "1.11.9",
    "immer": "10.0.2",
    "lodash-es": "4.17.21",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-router": "6.14.2",
    "react-router-dom": "6.14.2",
    "zustand": "4.4.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.2.1",
    "@storybook/addon-interactions": "7.2.1",
    "@storybook/addon-links": "7.2.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-styling": "1.3.5",
    "@storybook/blocks": "7.2.1",
    "@storybook/react": "7.2.1",
    "@storybook/react-vite": "7.2.1",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@total-typescript/ts-reset": "0.4.2",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/lodash-es": "4.17.8",
    "@types/node": "20.4.7",
    "@types/qs": "6.9.7",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "@vitejs/plugin-react": "4.0.4",
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/ui": "0.34.1",
    "autoprefixer": "10.4.14",
    "daisyui": "3.5.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "happy-dom": "10.7.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "lint-staged": "13.2.3",
    "madge": "6.1.0",
    "npm-check-updates": "16.10.17",
    "picocolors": "1.0.0",
    "postcss": "8.4.27",
    "prettier": "3.0.1",
    "prettier-plugin-tailwindcss": "0.4.1",
    "rimraf": "5.0.1",
    "rollup-plugin-visualizer": "5.9.2",
    "storybook": "7.2.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "typescript-plugin-css-modules": "5.0.1",
    "vite": "4.4.8",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-node-polyfills": "0.9.0",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-singlefile": "0.13.5",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.1"
  }
}
