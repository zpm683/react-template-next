{
  "name": "react-template-next",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "mock": "json-server -c .mock/config.json .mock/data.json",
    "lint": "eslint --fix src",
    "test": "vitest --ui",
    "e2e-test": "cypress open",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "build": "vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-circular": "npx madge --circular src",
    "output-dependencies": "npx madge --image dependencies.svg src",
    "remove-node_modules": "npx rimraf node_modules",
    "upgrade-node_modules": "npx npm-check-updates -u",
    "prebuild": "npm run check-circular && npm run lint",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix src"
    ],
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@hookform/resolvers": "5.1.1",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "@mui/x-date-pickers": "8.9.0",
    "ahooks": "3.9.0",
    "axios": "1.11.0",
    "clipboard": "2.0.11",
    "dayjs": "1.11.13",
    "immer": "10.1.1",
    "qs": "6.14.0",
    "radash": "12.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "7.61.1",
    "react-router": "7.7.1",
    "react-router-dom": "7.7.1",
    "react-scan": "0.4.3",
    "zod": "4.0.10",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@storybook/addon-links": "9.0.18",
    "@storybook/addon-onboarding": "9.0.18",
    "@storybook/addon-styling-webpack": "2.0.0",
    "@storybook/addon-themes": "9.0.18",
    "@storybook/react": "9.0.18",
    "@storybook/react-vite": "9.0.18",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "24.1.0",
    "@types/qs": "6.14.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react-swc": "3.11.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "cypress": "14.5.2",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-check-file": "3.3.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "happy-dom": "18.0.1",
    "husky": "9.1.7",
