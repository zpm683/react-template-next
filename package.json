    "react-error-boundary": "5.0.0",
    "react-hook-form": "7.54.2",
    "react-router": "7.4.0",
    "react-router-dom": "7.4.0",
    "react-scan": "0.3.2",
    "zod": "3.24.2",
    "zustand": "5.0.3"
